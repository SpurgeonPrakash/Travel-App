{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": ["airbnb-base", "plugin:node/recommended"],
  //   "plugins": ["prettier"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    // "prettier/prettier": "error",
    "func-names": "off"
  }
}
